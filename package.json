				"eslint": "^6.7.1",
				"eslint-config-prettier": "^6.7.0",
				"eslint-plugin-import": "^2.19.1",
				"husky": "^3.1.0",
				"jest": "^24.9.0",
				"nodemon": "^1.19.2",
				"prettier": "^1.15.3",
				"progress-bar-webpack-plugin": "^1.12.1",
				"supertest": "^4.0.2",
				"ts-jest": "^24.1.0",
				"ts-loader": "^6.1.0",
				"ts-node": "^8.6.2",
				"tsconfig-paths": "^3.9.0",
				"tslint": "^5.20.0",
				"typescript": "^3.6.3",
				"wait-on": "^3.2.0",
				"webpack": "^4.40.2",
				"webpack-cli": "^3.3.9",
				"webpack-loader-helper": "^1.0.0",
				"webpack-node-externals": "^1.7.2",
				"webpack-notifier": "^1.8.0"
		},
		"jest": {
				"moduleFileExtensions": [
						"js",
						"json",
						"ts"
				],
				"rootDir": "src",
				"testRegex": ".spec.ts$",
				"transform": {
						"^.+\\.(t|j)s$": "ts-jest"
				},
				"coverageDirectory": "../coverage",
				"testEnvironment": "node"
		}
}
